{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "rules": {
    "angular/di": [0, "function"],
    "angular/module-setter": 0,
    "angular/module-getter": 0,
    "angular/controller-as": 0,
    "no-console": 0,
    "angular/log": 0,
    "angular/on-watch": 0,
    "angular/controller-as-vm": 0
  },
  "env": {
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true
  }
}
